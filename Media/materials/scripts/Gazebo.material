material Gazebo/Turret
{
	technique
	{
		pass
		{
                       lighting off


                        ambient 1 1 1 1
			diffuse 1 1 1 1
			specular 0 0 0 0
			emissive 0 0 0

			alpha_rejection greater 128
			depth_write on

			texture_unit
			{
				texture turret_new.png
				tex_coord_set 0
				colour_op modulate
			}
		}
	}
}


