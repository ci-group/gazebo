QMainWindow
{
  background-color: #5d5d5d;
  background-image: url(:/images/mainwindow_bg.png);
  background-repeat: repeat-xy;
}

QMenuBar
{
  background-color: #202020;
  border-bottom-left-radius: 0px;
  border-bottom-right-radius: 10px;
  border: 1px solid #404040;
}

QMenuBar::item
{
  color: #808080;
  background: #202020;
  margin-top: 2px;
  margin-bottom: 2px;
  margin-left: 4px;
  margin-right: 4px;

  padding-top: 2px;
  padding-left: 2px;
  padding-right: 2px;
  padding-bottom: 2px;
}

QMenuBar::item:selected
{
  color: #f58113;
}

QMenu
{
  color: #808080;
  background-color: #202020;
}

QMenu::item:selected
{
  color: #f58113;
}

