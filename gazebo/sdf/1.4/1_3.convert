<convert name="gazebo">
  <convert name="world">
    <move>
      <from element="physics::update_rate"/>
      <to element="real_time_update_rate"/>
    </move>
    <move>
      <from element="physics::ode::dt"/>
      <to element="max_step_size"/>
    </move>
    <move>
      <from element="physics::bullet::dt"/>
      <to element="max_step_size"/>
    </move>
  </convert>
</convert> <!-- End SDF -->
