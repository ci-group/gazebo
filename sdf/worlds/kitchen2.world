    <include filename="bowl.model" model_name="bowl2" model_pose="-1.5 -2.5 0.867 0 0 0"/>
    <include filename="bowl.model" model_name="bowl3" model_pose="-1.0 -2.5 0.867 0 0 0"/>
    <include filename="bowl.model" model_name="bowl4" model_pose="-0.5 -2.5 0.867 0 0 0"/>
    <include filename="saucepan.model" model_name="saucepan" model_pose="-0.3 -0.16 0.9 0 0 -1.5707"/>
