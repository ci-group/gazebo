<gazebo version='1.0'>
  <model name="car" static="false">
    <link name="chassis">
      <origin pose="0 0 0.3 0 0 0"/>
      <inertial mass="1.0"/>
      <collision name="collision">
        <geometry>
          <box size="2.0 1.0 0.2"/>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <box size="2.0 1.0 0.2"/>
        </geometry>
      </visual>
    </link>

    <link name="front_left_wheel">
      <origin pose="0.8 0.6 0.15 0 1.5707 1.5707"/>

      <inertial mass="0.2"/>

      <collision name="collision">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </visual>
    </link>

    <link name="front_right_wheel">
      <origin pose="0.8 -0.6 0.15 0  1.5707 1.5707"/>

      <inertial mass="0.2"/>

      <collision name="collision">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </visual>
    </link>

    <link name="back_right_wheel">
      <origin pose="-0.8 -0.6 0.15 0 1.5707 1.5707"/>

      <inertial mass="0.2"/>

      <collision name="collision">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </visual>
    </link>

    <link name="back_left_wheel">
      <origin pose="-0.8 0.6 0.15 0 1.5707 1.5707"/>

      <inertial mass="0.2"/>

      <collision name="collision">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </collision>

      <visual name="visual">
        <geometry>
          <cylinder radius="0.3" length="0.1"/>
        </geometry>
      </visual>
    </link>

    <joint type="revolute2" name="front_left_joint">
      <parent link="chassis"/>
      <child link="front_left_wheel"/>
      <axis xyz="0 0 1"/>
      <axis2 xyz="0 1 0"/>
    </joint>

    <joint type="revolute2" name="front_right_joint">
      <parent link="chassis"/>
      <child link="front_right_wheel"/>
      <axis xyz="0 0 1"/>
      <axis2 xyz="0 1 0"/>
    </joint>

    <joint type="revolute" name="back_right_joint">
      <parent link="chassis"/>
      <child link="back_right_wheel"/>
      <axis xyz="0 1 0"/>
    </joint>

    <joint type="revolute" name="back_left_joint">
      <parent link="chassis"/>
      <child link="back_left_wheel"/>
      <axis xyz="0 1 0"/>
    </joint>

    <plugin filename="libVehiclePlugin.so" name="vehicle">
      <chassis>chassis</chassis>
      <front_left>front_left_joint</front_left>
      <front_right>front_right_joint</front_right>
      <back_left>back_left_joint</back_left>
      <back_right>back_right_joint</back_right>
    </plugin>
  </model> 
</gazebo>
