prefix=@CMAKE_INSTALL_PREFIX@
libdir=${prefix}/lib
includedir=${prefix}/include

Name: gazebo
Description: Gazebo Libraries
Version: @GAZEBO_VERSION@
Requires:
Libs: -Wl,-rpath,${libdir},-rpath,${prefix}/lib/gazebo-@GAZEBO_VERSION@/plugins -L${libdir} -L${prefix}/lib/gazebo-@GAZEBO_VERSION@/plugins -lgazebo_server 
CFlags: -I${includedir}/gazebo-@GAZEBO_VERSION@/gazebo
